{
  "name": "event-muster-server",
  "version": "0.0.1",
  "description": "A node.js server for assigning players to tables in multi-table gaming events.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d lib",
    "start": "nodemon --exec babel-node --require dotenv/config src/index.js",
    "serve": "node lib/index.js",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TecKnow/muster-tools.git"
  },
