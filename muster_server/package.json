{
  "name": "event-muster-server",
  "version": "0.0.1",
  "description": "A node.js server for assigning players to tables in multi-table gaming events.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d lib && cd ../muster_client && npm run build",
    "start-server": "nodemon --exec babel-node --require dotenv/config src/index.js",
    "start-client": "cd ../muster_client && npm start",
    "start-dev": "concurrently --kill-others-on-fail \"npm:start-server\" \"npm:start-client\"",
    "serve": "node lib/index.js",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TecKnow/muster-tools.git"
  },
  "author": "David P. Perkins",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/TecKnow/muster-tools/issues"
  },
  "homepage": "https://github.com/TecKnow/muster-tools#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.15",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.5.2",
    "concurrently": "^6.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.0",
    "jest": "^26.5.3",
    "nodemon": "^2.0.5",
    "prettier": "^2.2.1",
    "redux-devtools": "^3.7.0",
    "tmp-promise": "^3.0.2"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
    "express": "^4.17.1",
    "lodash": "^4.17.20",
    "redux-logger": "^3.0.6",
    "redux-socket.io": "^1.4.0",
    "sequelize": "^6.6.2",
    "socket.io": "^4.0.1",
    "sqlite3": "^5.0.2"
  }
}
