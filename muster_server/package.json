  "bugs": {
    "url": "https://github.com/TecKnow/muster-tools/issues"
  },
  "homepage": "https://github.com/TecKnow/muster-tools#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.1",
    "@babel/node": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-jest": "^26.5.2",
    "dotenv": "^8.2.0",
    "jest": "^26.5.3",
    "nodemon": "^2.0.5",
    "prettier": "2.1.2",
    "redux-devtools": "^3.7.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.4.0",
